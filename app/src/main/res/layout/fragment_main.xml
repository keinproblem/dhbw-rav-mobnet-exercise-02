<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WifiInfoFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Wifi State"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="112dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="5GHz Support"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Link Speed"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="54dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Current Frequency"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="93dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Ip Address"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="35dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="BSSID"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="74dp" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="SSID"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="74dp" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Adapter MAC"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="74dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rssi"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="74dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="111dp">

            <TextView
                android:id="@+id/wifiStateTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Unavailable"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="112dp" />

            <TextView
                android:id="@+id/fiveGhzSupportTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="35dp" />

            <TextView
                android:id="@+id/linkSpeedTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/frequencyTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="73dp" />

            <TextView
                android:id="@+id/ipAddrTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="54dp" />

            <TextView
                android:id="@+id/bssidTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="93dp" />

            <TextView
                android:id="@+id/ssidTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="93dp" />

            <TextView
                android:id="@+id/adapterMacTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="93dp" />

            <TextView
                android:id="@+id/rssiTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="196dp"
                tools:layout_editor_absoluteY="93dp" />
        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>